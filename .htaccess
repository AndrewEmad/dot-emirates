# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /dot-emirates/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /dot-emirates/index.php [L]
</IfModule>

# END WordPress





ErrorDocument 500 http://localhost/dot-emirates/error500/
